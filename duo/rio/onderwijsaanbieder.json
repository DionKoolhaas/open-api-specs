			],
			"type": "object"
		},
		"onderwijsaanbiederID": {
			"type": "string",
			"description": "Een betekenisloze identifier voor een onderwijsaanbieder in de Registratie Instellingen en Opleidingen.",
			"externalDocs": { 
				"url": "http://lod.onderwijsregistratie.nl/cat/cdm/def/onderwijsaanbiederID-v01",
				"description": "onderwijsaanbiederID-v01"
			},
			"maxLength": 7,
			"minLength": 7,
			"pattern": "^(\\d{3}A\\d{3})$"
		},
		"naam": {
			"type": "string",
			"description": "De naam die de onderwijsorganisatie in het maatschappelijk verkeer hanteert voor de organisatorische eenheid ",
			"externalDocs": { 
				"url": "http://lod.onderwijsregistratie.nl/cat/cdm/def/organisatorischeEenheidnaam-v01",
				"description": "organisatorischeEenheidnaam-v01"
			},
			"maxLength": 80
		},
		"internationaleNaam": {
			"type": "string",
			"description": "De naam voor internationaal gebruik.",
			"externalDocs": { 
				"url": "http://lod.onderwijsregistratie.nl/cat/cdm/def/internationaleNaam-v01",
				"description": "internationaleNaam-v01"
			},
			"maxLength": 225,
			"minLength": 1
		},
		"algemeenTelefoonnummer": {
			"type": "string",
			"description": "Het geheel aan cijfers en tekens waarmee wordt aangeduid hoe een persoon via telefonie kan worden bereikt",
			"externalDocs": { 
				"url": "http://lod.onderwijsregistratie.nl/cat/cdm/def/telefoonnummer-v02",
				"description": "telefoonnummer-v02"
			},
			"maxLength": 22,
			"pattern": "^((085)[-]?[0-9]{7})|((088)[-]?[0-9]{7})|(((0)[1-9]{2}[0-9][-]?[1-9][0-9]{5})|((\\+31|0|0031)[1-9][0-9][-]?[1-9][0-9]{6}))|((06)[-]?[1-9]{1}[0-9]{7})|(((\\+31|0|0031)6){1}[-]?[1-9]{1}[0-9]{7})|\\+(1\\d{4,14}|2[078]\\d{3,13}|3[0-469]\\d{3,13}|4[013-9]\\d{3,13}|5[1-8]\\d{3,13}|6[0-6]\\d{3,13}|7\\d{4,14}|8[1-469]\\d{3,13}|9[0-58]\\d{3,13}|[2-9]\\d{4,14})$"
		},
		"algemeenEmailadres": {
			"type": "array",
			"items": {
				"type": "string",
				"description": "Is een adres voor het ontvangen en verzenden van elektronische post.",
				"externalDocs": { 
					"url": "http://lod.onderwijsregistratie.nl/cat/cdm/def/emailadres-v05",
					"description": "emailadres-v05"
				},
				"maxLength": 256,
				"minLength": 4
				}
		},
		"algemeenWebadres": {
			"type": "string",
			"description": "De locatie op het World Wide Web waar documenten en andere objecten kunnen worden gevonden",
			"externalDocs": { 
				"url": "http://lod.onderwijsregistratie.nl/cat/cdm/def/uRL",
				"description": "uRL"
			},
			"maxLength": 320,
			"minLength": 0
		},
		"onderwijsmethode": {
			"type": "string",
			"description": "Is een vast weldoordachte manier van het geven van onderwijs.",
			"externalDocs": { 
				"url": "http://lod.onderwijsregistratie.nl/cat/cdm/def/onderwijsmethode-v01",
				"description": "onderwijsmethode-v01"
			},
			"maxLength": 70,
			"minLength": 1,
			"pattern": "^[A-Z0-9_]*$"
		},
		"eigenOnderwijsaanbiederSleutel": {
			"type": "array",
			"items": {
				"type": "string",
				"description": "Een identificerend kenmerk dat door een onderwijsorganisatie is bepaald voor een onderwijsaanbieder ten behoeve van uitwisseling in de keten.",
				"externalDocs": { 
					"url": "http://lod.onderwijsregistratie.nl/cat/cdm/def/onderwijsaanbiederExterneIdentificatie-v01",
					"description": "onderwijsaanbiederExterneIdentificatie-v01"
				},
				"maxLength": 60,
				"minLength": 1
				}
		},
		"inBedrijfdatum": {
			"type": "string",
			"description": "De datum waarop een organisatorische eenheid zoals een onderwijsaanbieder start. ",
			"externalDocs": { 
				"url": "http://lod.onderwijsregistratie.nl/cat/cdm/def/inBedrijfdatum-v01",
				"description": "inBedrijfdatum-v01"
			},
			"pattern": "^([0-9]{4}-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01]))$"
		},
		"uitBedrijfdatum": {
			"type": "string",
			"description": "De datum waarop een organisatorische eenheid zoals een onderwijsaanbieder niet meer bestaat.",
			"externalDocs": { 
				"url": "http://lod.onderwijsregistratie.nl/cat/cdm/def/uitBedrijfdatum-v01",
				"description": "uitBedrijfdatum-v01"
			},
			"pattern": "^([0-9]{4}-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01]))$"
		}
	},
	"required": [
		"_links",
		"onderwijsaanbiederID",
		"naam",
		"inBedrijfdatum"
	]
}
