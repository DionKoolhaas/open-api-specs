{
	"__comment__": "Gegenereerd op basis van commit#115 in het DUO_POC_Registratie Instellingen en Opleidingen (RIO) op 20190509 om 10:04. Gemaakt met template-jsonschema.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "NederlandsAdres JSON-Schema",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"_links": {
			"properties": {
				"self": {
					"description": "Absolute link naar deze resource",
					"properties": {
						"href": {
							"description": "URL van de link",
							"format": "uri",
							"type": "string"
						}
					},
					"required": [
						"href"
					],
					"type": "object"
				}
			},
			"required": [
				"self"
			],
			"type": "object"
		},
		"straatnaam": {
			"type": "string",
			"description": "http://lod.onderwijsregistratie.nl/cat/rio/sh/NederlandsAdres/straatnaam",
			"maxLength": 80,
			"minLength": 1
		},
		"huisnummer": {
			"type": "string",
			"description": "http://lod.onderwijsregistratie.nl/cat/rio/sh/NederlandsAdres/huisnummer",
			"maxLength": 3000,
			"pattern": "^\\d{0,5}$"
		},
		"huisnummertoevoeging": {
			"type": "string",
			"description": "http://lod.onderwijsregistratie.nl/cat/rio/sh/NederlandsAdres/huisnummertoevoeging",
			"maxLength": 6,
			"minLength": 1
		},
		"postcode": {
			"type": "string",
			"description": "http://lod.onderwijsregistratie.nl/cat/rio/sh/NederlandsAdres/postcode",
			"maxLength": 6,
			"pattern": "^([1-9]{1}[0-9]{3}[A-Z]{2})$"
		},
		"plaatsnaam": {
			"type": "string",
			"description": "http://lod.onderwijsregistratie.nl/cat/rio/sh/NederlandsAdres/plaatsnaam",
			"maxLength": 40,
			"minLength": 1
		}
	},
	"required": [
		"_links",
		"straatnaam",
		"huisnummer",
		"postcode",
		"plaatsnaam"
	]
}
