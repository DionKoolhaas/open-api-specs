			"type": "string",
			"description": "http://lod.onderwijsregistratie.nl/cat/cdm/def/onderwijsbestuurID-v01",
			"maxLength": 7,
			"minLength": 7,
			"pattern": "^(\\d{3}B\\d{3})$"
		},
		"naam": {
			"type": "string",
			"description": "http://lod.onderwijsregistratie.nl/cat/cdm/def/organisatorischeEenheidnaam-v01",
			"maxLength": 80
		},
		"internationaleNaam": {
			"type": "string",
