			"minLength": 1,
			"pattern": "^(\\d{9})?$"
		},
		"kVKnummer": {
			"type": "string",
			"description": "http://lod.onderwijsregistratie.nl/cat/cdm/def/kvK-nummer",
			"maxLength": 8,
			"minLength": 8,
			"pattern": "^(\\d{8})?$"
		},
		"inBedrijfdatum": {
			"type": "string",
			"description": "http://lod.onderwijsregistratie.nl/cat/cdm/def/inBedrijfdatum-v01",
			"pattern": "^([0-9]{4}-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01]))$"
		},
		"uitBedrijfdatum": {
			"type": "string",
			"description": "http://lod.onderwijsregistratie.nl/cat/cdm/def/uitBedrijfdatum-v01",
